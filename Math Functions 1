#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    float a;
    int sq,po,bas;
    scanf("%f",&a);
    scanf("%d",&sq);
    scanf("%d",&bas);
    scanf("%d",&po);
    printf("%d\n",(int)floor(a));
    printf("%d\n",(int)ceil(a));
    printf("%d\n",(int)sqrt(sq));
    printf("%d",(int)pow(bas,po));
    return 0;
}
