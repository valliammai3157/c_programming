#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    
    int a,b,c;
    scanf("%d\n%d\n%d\n",&a,&b,&c);
    if(a>b&&a>c)
        printf("a is largest then b and c");
    else if(b>a&&b>c)
        printf("b is largest then a and c");
    else
        printf("c is largest then a and b");
    return 0;
}

