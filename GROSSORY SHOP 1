#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int a4sheet,pen,pencil,eraser;
    scanf("%d",&a4sheet);
    scanf("%d",&pen);
    scanf("%d",&pencil);
    scanf("%d",&eraser);
    printf("Items Details\n");
    printf("A4sheet:%d\n",a4sheet);
    printf("Pen:%d\n",pen);
    printf("Pencil:%d\n",pencil);
    printf("Eraser:%d\n",eraser);
    return 0;
}
