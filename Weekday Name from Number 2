#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int week;
        scanf("%d\n",&week);
    switch(week){
        case 1:
          printf("Monday\n");
          break;
        case 2:
          printf("Tuesday\n");
          break;
        case 3:
          printf("Wednesday\n");
          break;
        case 4:
          printf("Thursday\n");
          break;
        case 5:
          printf("Friday\n");
          break;
        case 6:
          printf("Saturday\n");
          break;
        case 7:
          printf("Sunday\n");
          break;    
        default:
          printf("Enter a valid Input\n");
    }     
    return 0;
}
